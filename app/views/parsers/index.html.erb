<h1>Listing parsers</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Xpath</th>
      <th>URL</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @parsers.each do |parser| %>
      <tr>
        <td><%= parser.name %></td>
        <td><%= parser.xpath %></td>
        <td><%= parser.url %></td>
        <td><%= link_to 'Show', parser %></td>
        <td><%= link_to 'Edit', edit_parser_path(parser) %></td>
        <td><%= link_to 'Destroy', parser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parser', new_parser_path %>
