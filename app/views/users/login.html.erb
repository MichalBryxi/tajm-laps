<div class="row">
  <h1>Sign in</h1>
  <p>Login via Twitter is currently not working</p>
  <div class="col-xs-6">
    <%= link_to user_omniauth_authorize_path(:twitter), class: 'btn btn-info btn-lg btn-block', disabled: 'disabled' do %>
      <%= icon('twitter') %>
      &nbsp;&nbsp;&nbsp;
      Twitter
    <% end %>
  </div>
  <div class="col-xs-6">
    <%= link_to user_omniauth_authorize_path(:facebook), class: 'btn btn-primary btn-lg btn-block' do %>
      <%= icon('facebook') %>
      &nbsp;&nbsp;&nbsp;
      facebook
    <% end %>
  </div>
</div>
